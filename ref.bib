@article{karamanDifferentPathsModern2013,
  ids = {karamanDifferentPathsModern2013a},
  title = {Different {{Paths}} to the {{Modern State}} in {{Europe}}: {{The Interaction Between Warfare}}, {{Economic Structure}}, and {{Political Regime}}},
  author = {KARAMAN, K KIVANÇ and family=PAMUK, given=ŞEVKET, given-i=ŞEVKET},
