@article{beatonHelpingLessFortunate2005,
  title = {Helping the Less Fortunate: {{A}} Predictive Model of Collective Action.},
  author = {Beaton, Ann M and Deveau, Mylène},
  date = {2005},
  journaltitle = {Journal of Applied Social Psychology},
  volume = {35},
  number = {8},
  pages = {1609--1629},
  publisher = {Blackwell Publishing},
  location = {Beaton, Ann M.: Ecole de Psychologie, Universite de Moncton, Moncton, NB, Canada, E1A 3E9, beatona@umoncton.ca},
  issn = {1559-1816(Electronic),0021-9029(Print)},
  doi = {10.1111/j.1559-1816.2005.tb02187.x},
  abstract = {A model of factors that lead members of an advantaged group to perform collective action intended to help a disadvantaged out-group is tested. A distinction is made between easy and demanding collective activities. Two different routes are expected to lead to collective action. First, an indirect link is proposed between a common in-group identity and collective action. This link is believed to be mediated by perceived mobilization resources. Second, the link between relative deprivation on behalf of others (RDBO) and collective action will vary according to the type of behavior considered. While, RDBO is expected to be directly associated with easy collective behavior, the link between RDBO and demanding collective activities is mediated through perceived mobilization resources. In turn, perceived resources are linked to both forms of collective behaviors. The predicted model is tested among 273 undergraduate respondents. Findings point to the relevance of all factors considered and the pivotal role of perceived mobilization resources on collective action. (PsycINFO Database Record (c) 2016 APA, all rights reserved)},
  keywords = {*Assistance (Social Behavior),*Collective Behavior,*Disadvantaged,*Ingroup Outgroup,*Resource Allocation,Behavior,Group Dynamics}
}

