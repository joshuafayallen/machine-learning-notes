@article{zhangPartiallyWhollyOverlapping2015,
  title = {Partially and {{Wholly Overlapping Networks}}: {{The Evolutionary Dynamics}} of {{Social Dilemmas}} on {{Social Networks}}},
  author = {Zhang, Yanlong},
  date = {2015},
  journaltitle = {Computational Economics},
  volume = {46},
  number = {1},
  pages = {1--14},
  doi = {10.1007/s10614-014-9447-6},
  abstract = {It has been widely accepted that the evolution of cooperation in social\textbackslash ndilemmas is likely to have simultaneous effects on different levels of\textbackslash nnetworks-that is, overlapping networks. In this scenario, we apply the\textbackslash nevolutionary dynamics of social dilemmas (the Prisoner's dilemma and the\textbackslash nStag Hunt) to two levels of networks, playing network interaction game\textbackslash n(scale-free networks) and imitating the strategies of dynamic learning\textbackslash nnetworks by concentrating on the levels of dynamically overlapping\textbackslash nlevels in both networks. It was found that the higher level of\textbackslash ndynamically overlap can promote the emergence of cooperation in the\textbackslash nPrisoner's Dilemma and the Stag Hunt. When networks overlap dynamically,\textbackslash nit leads to cooperation in the Stag Hunt; when the networks do not\textbackslash noverlap, cooperation will not occur in the Prisoner's dilemma.\textbackslash nSpecifically, if two kinds of networks in the real world truly\textbackslash ndynamically overlap at least partially, in some sense, the\textbackslash nself-organizing phenomenon arising out of the overlapping effect could\textbackslash nsolve ``coordination failure\{''\} in the market without external\textbackslash nenforcement mechanisms, which contradicts conventional intervention\textbackslash npolices.},
  keywords = {Evolutioanry dyanmics,Overlapping networks,Self-organizing phenomenon,Social dilemmas}
}

@article{zhaoNotePosttreatmentSelection2021,
  title = {A {{Note}} on {{Posttreatment Selection}} in {{Studying Racial Discrimination}} in {{Policing}}},
  author = {family=ZHAO, given=QINGYUAN, given-i=QINGYUAN and KEELE, LUKE J and SMALL, DYLAN S and JOFFE, MARSHALL M},
  date = {2021},
  journaltitle = {American Political Science Review},
  edition = {2021/07/26},
  pages = {1--14},
  publisher = {Cambridge University Press},
  issn = {0003-0554},
  doi = {10.1017/S0003055421000654},
  url = {https://www.cambridge.org/core/article/note-on-posttreatment-selection-in-studying-racial-discrimination-in-policing/87DDBF96EFFCF3BEC97530C67AAA4031},
  abstract = {We discuss some causal estimands that are used to study racial discrimination in policing. A central challenge is that not all police–civilian encounters are recorded in administrative datasets and available to researchers. One possible solution is to consider the average causal effect of race conditional on the civilian already being detained by the police. We find that such an estimand can be quite different from the more familiar ones in causal inference and needs to be interpreted with caution. We propose using an estimand that is new for this context—the causal risk ratio, which has more transparent interpretation and requires weaker identification assumptions. We demonstrate this through a reanalysis of the NYPD Stop-and-Frisk dataset. Our reanalysis shows that the naive estimator that ignores the posttreatment selection in administrative records may severely underestimate the disparity in police violence between minorities and whites in these and similar data.}
}

