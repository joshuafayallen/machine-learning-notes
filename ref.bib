@article{hagleGoodnessofFitMeasuresProbit1992,
  ids = {hagleGoodnessofFitMeasuresProbit1992a},
  title = {Goodness-of-{{Fit Measures}} for {{Probit}} and {{Logit}}},
  author = {Hagle, Timothy M. and Mitchell, Glenn E.},
  date = {1992},
  journaltitle = {American Journal of Political Science},
  volume = {36},
  number = {3},
  eprint = {2111590},
  eprinttype = {jstor},
  pages = {762--784},
  publisher = {[Midwest Political Science Association, Wiley]},
  issn = {00925853, 15405907},
  doi = {10.2307/2111590},
  url = {http://www.jstor.org/stable/2111590},
  urldate = {2021-07-22},
  abstract = {[Some regression analysts find the R\textsuperscript{2} statistic to be of little utility. Others use it extensively while evaluating model performance. One of the shortcomings of probit and logit analyses is the lack of an analog to the Ordinary Least Squares (OLS) R\textsuperscript{2} statistic. To aid in the evaluation of model performance, several pseudo-R\textsuperscript{2}s have been proposed. Dichotomizing a continuous interval-level variable results in distortions due to a loss of information. We do not know, however, the degree to which these distortions affect the pseudo-R\textsuperscript{2}s vis-a-vis the OLS R\textsuperscript{2} that is based on the underlying continuous dependent variable. In this study we use simulation techniques to compare four common pseudo-R\textsuperscript{2}s for probit and logit with the R\textsuperscript{2} that would be obtained under OLS regression. After making a correction to one of the measures, two of them perform quite well, comparing favorably with the OLS R\textsuperscript{2}. The choice between them may be simply a matter of availability and ease of use.]}
}

