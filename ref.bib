  title = {Time Is of the {{Essence}}: {{Event History Models}} in {{Political Science}}},
  author = {Box-Steffensmeier, Janet M and Jones, Bradford S},
  date = {1997},
  journaltitle = {American Journal of Political Science},
  volume = {41},
