@article{cusackEconomicInterestsOrigins2007,
  title = {Economic {{Interests}} and the {{Origins}} of {{Electoral Systems}}},
  author = {CUSACK, THOMAS R. and family=IVERSEN, given=TORBEN, given-i=TORBEN and family=SOSKICE, given=DAVID, given-i=DAVID},
  date = {2007},
  journaltitle = {American Political Science Review},
  edition = {2007/07/26},
