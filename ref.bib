@article{schuurmanClausewitzNewWars2010,
  title = {Clausewitz and the “{{New Wars}}” {{Scholars}}},
  author = {Schuurman, Bart},
  date = {2010},
  journaltitle = {Parameters},
  volume = {40},
  number = {1},
  pages = {89--100},
  issn = {0031-1723},
  abstract = {The "new wars" school of thought has contributed significantly to understanding why conventional military superiority has limited valye in civil wars or counterinsurgencies. Unfortunately, some of these theorists have also attempted to fundamentally change the way we think about war in general. This approach has led to several well-entrenced misunderstandings regarding war's fundamental characteristics and the relationship between contemporary and historical conflicts. This article will shed some light on these misunderstandings and show the faulty reasoning upon which they are based. By doing so the author hopes to make a contribution to the development of a more nuanced and robust intellectual framework that can be used to study historical and contemporary warfare.}
}

@article{schuurmanEndLoneWolf2018,
