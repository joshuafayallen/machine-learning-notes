@article{bonillaWhichIdentityFrames2020,
  title = {Which {{Identity Frames Boost Support}} for and {{Mobilization}} in the \#{{BlackLivesMatter Movement}}? {{An Experimental Test}}},
  author = {Bonilla, Tabitha and Tillery, Alvin B},
  date = {2020},
  journaltitle = {American Political Science Review},
  edition = {2020/07/30},
  volume = {114},
  number = {4},
  pages = {947--962},
  publisher = {Cambridge University Press},
  doi = {10.1017/S0003055420000544},
  url = {https://www.cambridge.org/core/article/which-identity-frames-boost-support-for-and-mobilization-in-the-blacklivesmatter-movement-an-experimental-test/AD0E73AF21C32FE03F1C0897456CC5C5},
  abstract = {The Black Lives Matter (BLM) movement has organized hundreds of disruptive protests in American cities since 2013 (Garza ; Harris ; Taylor ). The movement has garnered considerable attention from the U.S. media and is well recognized by the U.S. public (Horowitz and Livingston ; Neal ). Social movement scholars suggest that such robust mobilizations are typically predicated on clear social movement frames (Benford and Snow ; Snow et al. ). Tillery () has identified several distinct message frames within the social media communications of BLM activists. In this paper, we use a survey experiment to test the effect of three of these frames—Black Nationalist, Feminist, and LGBTQ+ Rights—on the mobilization of African Americans. We find that exposure to these frames generates differential effects on respondents’ willingness to support, trust, canvass, and write representatives about the Black Lives Matter movement. These findings raise new questions about the deployment of intersectional messaging strategies within movements for racial justice.}
}

