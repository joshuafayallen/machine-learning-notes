@article{bucklesInternalOppositionDynamics2019,
  title = {Internal {{Opposition Dynamics}} and {{Restraints}} on {{Authoritarian Control}}},
  author = {Buckles, Grant T},
  date = {2019},
  journaltitle = {British Journal of Political Science},
  edition = {2017/07/11},
  volume = {49},
  number = {3},
