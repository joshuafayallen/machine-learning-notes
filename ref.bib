@incollection{Judt:2002,
  title = {The Past Is Another Country: Myth and Memory in Post-War {{Europe}}},
  booktitle = {Memory and {{Power}} in {{Post-War Europe}}: {{Studies}} in the {{Presence}} of the {{Past}}},
