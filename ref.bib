  title = {Do Significant Terrorist Attacks Increase the Risk of Further Attacks?},
  author = {Jenkins, Brian M. and Wills, Henry and Han, Bing},
  date = {2016},
  journaltitle = {Rand Corp},
  abstract = {About This Perspective\textbackslash r\textbackslash nRAND researchers examine the historical record of terrorism in the United \textbackslash rStates and Europe between 1970 and 2013 to determine whether a signifi-\textbackslash rcant terrorist attack somehow inspires other attacks and whether terrorist \textbackslash rattacks can be anticipated statistically. Funding for this study was provided \textbackslash rby philanthropic contributions from RAND supporters and income from \textbackslash roperations. The authors thank Fred Gerstell, in particular, for his generosity.\textbackslash rThis research was conducted within the Homeland Security and \textbackslash rDefense Center (HSDC), which conducts analysis to prepare and protect \textbackslash rcommunities and critical infrastructure from natural disasters and terror-\textbackslash rism. Center projects examine a wide range of risk-management prob-\textbackslash rlems, including coastal and border security, emergency preparedness \textbackslash rand response, defense support to civil authorities, transportation security, \textbackslash rdomestic intelligence, and technology acquisition. Center clients include the \textbackslash rU.S. Department of Homeland Security, the U.S. Department of Defense, \textbackslash rthe U.S. Department of Justice, and other organizations charged with secu-\textbackslash rrity and disaster preparedness, response, and recovery.\textbackslash rHSDC is a joint center of two research divisions: RAND Justice, Infra-\textbackslash rstructure, and Environment and the RAND National Security Research Divi-\textbackslash rsion. RAND Justice, Infrastructure, and Environment is dedicated to improving \textbackslash rpolicy and decisionmaking in a wide range of policy domains, including \textbackslash rcivil and criminal justice, infrastructure protection and homeland security, \textbackslash rtransportation and energy policy, and environmental and natural resource \textbackslash rpolicy. The RAND National Security Research Division conducts research \textbackslash rand analysis for all national security sponsors other than the U.S. Air Force \textbackslash rand the Army. The division includes the National Defense Research Institute, \textbackslash ra federally funded research and development center whose sponsors include \textbackslash rthe Office of the Secretary of Defense, the Joint Staff, the Unified Combatant \textbackslash rCommands, the defense agencies, and the U.S. Department of the Navy. The \textbackslash rNational Security Research Division also conducts research for the U.S. intel-\textbackslash rligence community and the ministries of defense of U.S. allies and partners.\textbackslash rQuestions or comments about this report should be sent to the project \textbackslash rleader, Henry H. Willis, at hwillis@rand.org. For more information about the \textbackslash rHomeland Security and Defense Center, see http://www.rand.org/hsdc or \textbackslash rcontact the director at hsdc@rand.org.}
}

@article{JenneHawkinsSilva:2021,
  ids = {JenneHawkinsSilva:2021a},
  title = {Mapping {{Populism}} and {{Nationalism}} in {{Leader Rhetoric Across North America}} and {{Europe}}},
  author = {Jenne, Erin K. and Hawkins, Kirk A. and Silva, Bruno Castanho},
  date = {2021-06-01},
  journaltitle = {Studies in Comparative International Development},
  shortjournal = {Studies in Comparative International Development},
  volume = {56},
  number = {2},
  pages = {170--196},
  issn = {1936-6167},
  doi = {10.1007/s12116-021-09334-9},
  url = {https://doi.org/10.1007/s12116-021-09334-9},
  abstract = {We conceptualize populism and nationalism as vertical and horizontal discursive frames of sovereignty, and we investigate the prevalence of these frames in the speeches of chief executives (presidents and prime ministers) in Europe and North America to assess whether these discourses are on the rise at the highest levels of government. To do so, we compile an original database of leader speeches, measuring both discourses using a technique called holistic grading. We find that neither populism nor nationalism is on the rise across Europe and North America over the past twenty years; instead, the rise is concentrated in sub-regions and specific countries. We also find that populism and nationalism are highly but imperfectly correlated in leaders’ speeches in the corpus as a whole, but that populism is far less common in the speeches of western leaders. In the penultimate section, we use a selection of speech vignettes to demonstrate that state leaders employ populism to counter political opponents, nationalism to counter hostile nations, and a combination to mobilize against conjoined threats from above and beyond the “people-nation.”}
}

