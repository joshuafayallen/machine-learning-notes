@article{barfieldProblemsEstablishingLegitimacy2004,
  ids = {barfieldProblemsEstablishingLegitimacy2004a},
  title = {Problems in Establishing Legitimacy in {{Afghanistan}}},
  author = {Barfield, Thomas J.},
  date = {2004},
