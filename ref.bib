@article{canettiCollectiveTraumaLab2018,
  ids = {canettiCollectiveTraumaLab2018a},
  title = {Collective {{Trauma From}} the {{Lab}} to the {{Real World}}: {{The Effects}} of the {{Holocaust}} on {{Contemporary Israeli Political Cognitions}}},
  author = {Canetti, Daphna and Hirschberger, Gilad and Rapaport, Carmit and Elad-Strenger, Julia and Ein-Dor, Tsachi and Rosenzveig, Shifra and Pyszczynski, Tom and Hobfoll, Stevan E.},
