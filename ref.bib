@article{humphreysMixingMethodsBayesian2015,
  title = {Mixing {{Methods}}: {{A Bayesian Approach}}},
  author = {family=HUMPHREYS, given=MACARTAN, given-i=MACARTAN and JACOBS, ALAN M},
  date = {2015},
  journaltitle = {American Political Science Review},
  edition = {2015/12/23},
  volume = {109},
  number = {4},
  pages = {653--673},
  publisher = {Cambridge University Press},
  doi = {10.1017/S0003055415000453},
  url = {https://www.cambridge.org/core/article/mixing-methods-a-bayesian-approach/BB1DFC2FDA3D7F2224F3341042FEA5F4},
  abstract = {We develop an approach to multimethod research that generates joint learning from quantitative and qualitative evidence. The framework—Bayesian integration of quantitative and qualitative data (BIQQ)—allows researchers to draw causal inferences from combinations of correlational (cross-case) and process-level (within-case) observations, given prior beliefs about causal effects, assignment propensities, and the informativeness of different kinds of causal-process evidence. In addition to posterior estimates of causal effects, the framework yields updating on the analytical assumptions underlying correlational analysis and process tracing. We illustrate the BIQQ approach with two applications to substantive issues that have received significant quantitative and qualitative treatment in political science: the origins of electoral systems and the causes of civil war. Finally, we demonstrate how the framework can yield guidance on multimethod research design, presenting results on the optimal combinations of qualitative and quantitative data collection under different research conditions.}
}

@article{humphreysWhoFightsDeterminants2008,
  title = {Who {{Fights}}? {{The Determinants}} of {{Participation}} in {{Civil War}}},
  author = {Humphreys, Macartan and Weinstein, Jeremy M},
  date = {2008-04},
  journaltitle = {American Journal of Political Science},
  volume = {52},
  number = {2},
  pages = {436--455},
  publisher = {John Wiley \& Sons, Ltd (10.1111)},
  doi = {10.1111/j.1540-5907.2008.00322.x},
  url = {https://doi.org/10.1111/j.1540-5907.2008.00322.x},
  abstract = {A range of seemingly rival theories attempt to explain why some individuals take extraordinary risks by choosing to participate in armed conflict. To date, however, competing accounts have typically not been grounded in systematic, empirical studies of the determinants of participation. In this article, we begin to fill this gap through an examination of the determinants of participation in insurgent and counterinsurgent factions in Sierra Leone's civil war. We find some support for all of the competing theories, suggesting that the rivalry between them is artificial and that theoretical work has insufficiently explored the interaction of various recruitment strategies. At the same time, the empirical results challenge standard interpretations of grievance-based accounts of participation, as poverty, a lack of access to education, and political alienation predict participation in both rebellion and counterrebellion. Factors that are traditionally seen as indicators of grievance or frustration may instead proxy a for more general susceptibility to engage in violent action or a greater vulnerability to political manipulation by elites.}
}

