@article{abbottWhyStatesAct1998,
  title = {Why {{States Act}} through {{Formal International Organizations}}},
  author = {Abbott, Kenneth W and Snidal, Duncan},
  date = {1998-02},
  journaltitle = {Journal of Conflict Resolution},
  volume = {42},
  number = {1},
  pages = {3--32},
  publisher = {SAGE Publications Inc},
  doi = {10.1177/0022002798042001001},
  url = {https://doi.org/10.1177/0022002798042001001},
  abstract = {States use formal international organizations (IOs) to manage both their everyday interactions and more dramatic episodes, including international conflicts. Yet, contemporary international theory does not explain the existence or form of IOs. This article addresses the question of why states use formal organizations by investigating the functions IOs perform and the properties that enable them to perform those functions. Starting with a rational-institutionalist perspective that sees IOs as enabling states to achieve their ends, the authors examine power and distributive questions and the role of IOs in creating norms and understanding. Centralization and independence are identified as the key properties of formal organizations, and their importance is illustrated with a wide array of examples. IOs as community representatives further allow states to create and implement community values and enforce international commitments.}
}

@article{abdelgadirPoliticalSecularismMuslim2020,
  title = {Political {{Secularism}} and {{Muslim Integration}} in the {{West}}: {{Assessing}} the {{Effects}} of the {{French Headscarf Ban}}},
  author = {family=ABDELGADIR, given=AALA, given-i=AALA and family=FOUKA, given=VASILIKI, given-i=VASILIKI},
  date = {2020},
  journaltitle = {American Political Science Review},
  edition = {2020/05/12},
  volume = {114},
  number = {3},
  pages = {707--723},
  publisher = {Cambridge University Press},
  issn = {0003-0554},
  doi = {10.1017/S0003055420000106},
  url = {https://www.cambridge.org/core/article/political-secularism-and-muslim-integration-in-the-west-assessing-the-effects-of-the-french-headscarf-ban/2934B2DD5336FF53B8881F3F0C506B41},
  abstract = {In response to rising immigration flows and the fear of Islamic radicalization, several Western countries have enacted policies to restrict religious expression and emphasize secularism and Western values. Despite intense public debate, there is little systematic evidence on how such policies influence the behavior of the religious minorities they target. In this paper, we use rich quantitative and qualitative data to evaluate the effects of the 2004 French headscarf ban on the socioeconomic integration of French Muslim women. We find that the law reduces the secondary educational attainment of Muslim girls and affects their trajectory in the labor market and family composition in the long run. We provide evidence that the ban operates through increased perceptions of discrimination and that it strengthens both national and religious identities.}
}

@article{abdouramanConflitFrontalierCamerounNigeria2008,
  title = {Le Conflit Frontalier {{Cameroun-Nigeria}} Dans Le Lac {{Tchad}} : Les Enjeux de l'île de {{Darak}}, Disputée et Partagée},
  author = {Abdouraman, Halirou},
  date = {2008},
  journaltitle = {Cultures \& Conflits},
  volume = {72},
  number = {4},
  pages = {57--76},
  publisher = {L'Harmattan},
  url = {https://www.cairn-int.info/load_pdf.php?ID_ARTICLE=E_CC_072_0057},
  abstract = {Lake Chad has been, since the early 1980s, an object of conflict between the countries sharing its resources, namely Cameroon, Niger, Nigeria and Chad. These disputes derive from climatic variations, in particular severe and constant droughts, which have caused the progressive diminution of the lake. The reduction of the surface of this continental water reserve has entailed the ‘migration’ of fish and the emergence of islands which constitute rich agricultural and pastoral lands. The control of these resources, in an environment rendered increasingly difficult due to human and natural processes, has become the main stake of the border conflict between Cameroon and Nigeria in the Darak region. However, the long-established trans-border relations between the populations living in the region, are currently superseding the geopolitical and geostrategical opposition between these two states.},
  langid = {english}
}

