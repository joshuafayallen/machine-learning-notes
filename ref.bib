@article{homolaIdeologyBeholderHow2022,
  ids = {homolaIdeologyBeholderHow2022a},
  title = {Through the Ideology of the Beholder: How Ideology Shapes Perceptions of Partisan Groups},
  author = {Homola, Jonathan and Rogowski, Jon C. and Sinclair, Betsy and Torres, Michelle and Tucker, Patrick D. and Webster, Steven W.},
  date = {2022},
  journaltitle = {Political Science Research and Methods},
  edition = {2022/02/24},
  pages = {1--18},
  publisher = {Cambridge University Press},
  issn = {2049-8470},
  doi = {10.1017/psrm.2022.4},
  url = {https://www.cambridge.org/core/article/through-the-ideology-of-the-beholder-how-ideology-shapes-perceptions-of-partisan-groups/ED82FD533B701B20D063FE4457A79821},
  abstract = {Growing attitudinal and affective differences across party lines and increasing social polarization are often attributed to the strengthening of partisanship as a social identity. Scholars have paid less attention to personal preferences as a contributor to these phenomena. Our focus is on how citizens’ policy beliefs—their operational ideologies—are associated with their views of partisan groups. We examine our perspective with two studies. In the first, we find that the attribution of ideologically extreme political views to an individual's peer significantly reduces interest in interpersonal interaction but find limited evidence that partisan group membership alone induces social polarization. In the second, we show that citizens’ policy views are strongly associated with their perceptions of their own partisan group as well as their counterpartisans. Together, our results have important implications for understanding the consequences of increased polarization and partisan antipathy in contemporary politics.},
  keywords = {American politics,political psychology,public opinion}
}

@article{homolaLegaciesThirdReich2020,
  title = {Legacies of the {{Third Reich}}: {{Concentration Camps}} and {{Out-group Intolerance}}},
  author = {Homola, Jonathan and Pereira, Miguel and Tavits, Margit},
  date = {2020},
  journaltitle = {American Political Science Review},
  edition = {2020/01/27},
  volume = {114},
  number = {2},
  pages = {573--590},
  publisher = {Cambridge University Press},
  doi = {10.1017/S0003055419000832},
  url = {https://www.cambridge.org/core/article/legacies-of-the-third-reich-concentration-camps-and-outgroup-intolerance/EDA004EAEFCA2DFB0C6FBFBE2D50444D},
  abstract = {We explore the long-term political consequences of the Third Reich and show that current political intolerance, xenophobia, and voting for radical right-wing parties are associated with proximity to former Nazi concentration camps in Germany. This relationship is not explained by contemporary attitudes, the location of the camps, geographic sorting, the economic impact of the camps, or their current use. We argue that cognitive dissonance led those more directly exposed to Nazi institutions to conform with the belief system of the regime. These attitudes were then transmitted across generations. The evidence provided here contributes both to our understanding of the legacies of historical institutions and the sources of political intolerance.}
}

