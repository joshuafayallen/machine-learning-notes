@article{MELLONPROSSER:2024,
  title = {Regularized {{Regression Can Reintroduce Backdoor Confounding}}: {{The Case}} of {{Mass Polarization}}},
  author = {family=MELLON, given=JONATHAN, given-i=JONATHAN and family=PROSSER, given=CHRISTOPHER, given-i=CHRISTOPHER},
  date = {2024},
  journaltitle = {American Political Science Review},
  edition = {2024/10/31},
  pages = {1--9},
  publisher = {Cambridge University Press},
  issn = {0003-0554},
  doi = {10.1017/S0003055424000935},
  url = {https://www.cambridge.org/core/product/3806AC4212AE06432DA309F07890C486},
  abstract = {Regularization can improve statistical estimates made with highly correlated data. However, any regularization procedure embeds assumptions about the data generating process that can have counterintuitive consequences when those assumptions are untenable. We show that rather than simply shrinking estimates, regularization can reopen backdoor causal paths, inflating the estimates of some effects, and in the wrong circumstances, even reversing their direction. Recently, Cavari and Freedman (2023), argued that declining cooperation rates in surveys have inflated measures of mass polarization. We show that this finding is driven by large penalty terms in their regularized regressions, which leads to the estimates being confounded with time. Alternative methods do not show a clear positive or negative effect of declining cooperation on estimated levels of mass polarization.}
}

