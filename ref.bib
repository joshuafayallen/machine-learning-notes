@article{windischHeadhuntingExtremistOrganizations2018,
  title = {Headhunting {{Among Extremist Organizations}}: {{An Empirical Assessment}} of {{Talent Spotting}}},
  author = {Windisch, Steven and Logan, Michael K and Scott Ligon, Gina},
