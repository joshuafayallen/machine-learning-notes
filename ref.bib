  title = {{{BAYESIAN ANALYSIS FOR POLITICAL RESEARCH}}},
  author = {Jackman, Simon},
  date = {2004-05-17},
  journaltitle = {Annual Review of Political Science},
  shortjournal = {Annu. Rev. Polit. Sci.},
  volume = {7},
  number = {1},
  pages = {483--505},
  publisher = {Annual Reviews},
  issn = {1094-2939},
