@article{hightonSelfreportedProxyreportedVoter2005,
  title = {Self-Reported versus Proxy-Reported Voter Turnout in the Current Population Survey},
  author = {Highton, Benjamin},
  date = {2005},
  journaltitle = {Public Opinion Quarterly},
  volume = {69},
