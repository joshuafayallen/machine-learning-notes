@article{carlinPitfallPeaceFARC2020,
  title = {Pitfall to {{Peace}}: {{FARC}}’s {{Political Participation}} and {{Mass Support}} for {{Peace Talks}} in {{Colombia}}},
  author = {Carlin, Ryan E. and Love, Gregory J. and McCoy, Jennifer L. and Subotić, Jelena},
