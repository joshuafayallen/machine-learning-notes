@article{bergerExtremistConstructionIdentity2017,
  title = {Extremist {{Construction}} of {{Identity}}: {{How Escalating Demands}} for {{Legitimacy Shape}} and {{Define In-Group}} and {{Out-Group Dynamics}}},
