}

@article{carlosPoliticsMundane2021,
  title = {The {{Politics}} of the {{Mundane}}},
  author = {CARLOS, ROBERTO F.},
  date = {2021},
  journaltitle = {American Political Science Review},
  edition = {2021/04/12},
  volume = {115},
  number = {3},
