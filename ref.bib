@article{dardenIntegrityCorruptStates2008,
  title = {The {{Integrity}} of {{Corrupt States}}: {{Graft}} as an {{Informal State Institution}}},
  author = {Darden, Keith},
  date = {2008-03-01},
  journaltitle = {Politics \& Society},
  shortjournal = {Politics \& Society},
  volume = {36},
  number = {1},
  pages = {35--59},
  publisher = {SAGE Publications Inc},
  issn = {0032-3292},
  doi = {10.1177/0032329207312183},
  url = {https://doi.org/10.1177/0032329207312183},
  urldate = {2021-06-02},
  abstract = {This article argues that corrupt practices such as bribery and embezzlement, which scholars have previously assumed to be evidence of the breakdown of the state, may reinforce the state's administrative hierarchies under certain conditions. Drawing on a cross-national analysis of 132 countries and a detailed examination of the informal institutions of official graft in Ukraine, the article finds that where graft is systematically tracked, monitored, and granted by state leaders as an informal payment in exchange for compliance, it provides both an added incentive to obey leaders' directives and the potent sanction of criminal prosecution in the event of disobedience. Where graft is informally institutionalized in this way, it provides the basis for state organizations that are effective at collecting taxes, maintaining public order, and repressing political opposition but that may undermine the development of liberal politics.}
}

@article{Dargent:2009,
