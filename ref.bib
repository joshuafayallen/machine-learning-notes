@article{mahoneyLogicProcessTracing2012,
  ids = {mahoneyLogicProcessTracing2012a},
  title = {The {{Logic}} of {{Process Tracing Tests}} in the {{Social Sciences}}},
  author = {Mahoney, James},
  date = {2012-03-02},
  journaltitle = {Sociological Methods \& Research},
  shortjournal = {Sociological Methods \& Research},
  volume = {41},
  number = {4},
